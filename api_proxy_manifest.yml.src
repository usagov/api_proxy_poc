applications:
  - name: api-proxy
    memory: 256M
    instances: 1
    buildpacks:
      - python_buildpack
    routes:
    - route: ${ORG}-${SPACE_NODOT}-api-proxy.apps.internal
    - route: ${ORG}-${SPACE_NODOT}-api-proxy.app.cloud.gov
    env:
      API_ENDPOINT: ${API_ENDPOINT}
      API_KEY: ${API_KEY}
